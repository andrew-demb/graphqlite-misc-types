{
  "name" : "freshcells/graphqlite-misc-types",
  "description" : "A collection of non-standard GraphQL types to be used with GraphQLite.",
  "keywords" : [
    "graphql",
    "graphqlite",
    "types"
  ],
  "homepage" : "https://github.com/freshcells/graphqlite-misc-types",
  "type" : "library",
  "license" : "MIT",
  "authors" : [
    {
      "name" : "Ivo Bathke",
      "email" : "ivo.bathke@freshcells.de"
    },
    {
    "name" : "David NÃ©grier",
    "email" : "d.negrier@thecodingmachine.com",
    "homepage" : "https://thecodingmachine.io"
  }
  ],
  "require" : {
    "php" : ">=7.4",
    "thecodingmachine/graphqlite" : "^4",
    "webonyx/graphql-php": "^0.13.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0.0",
    "phpstan/phpstan": "^1.8.2",
    "symfony/cache": "^4.2.7",
    "php-coveralls/php-coveralls": "^2.1"
  },
  "scripts": {
    "test": "phpunit",
    "phpstan": "phpstan analyse -c phpstan.neon"
  },
  "autoload" : {
    "psr-4" : {
      "TheCodingMachine\\GraphQLite\\Types\\" : "src/"
    }
  },
  "autoload-dev" : {
    "psr-4" : {
      "TheCodingMachine\\GraphQLite\\Types\\" : "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
